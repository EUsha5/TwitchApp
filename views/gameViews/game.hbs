<div class="game-page">
<h3 class="game-title">List of Games</h3>
{{#each listOfGames}}
<div class="game-list">
  <a href="/game/{{this.id}}">{{this.name}}</a>
  <br>
    <img class="list-image" src="{{this.image}}">
    <br>
</div>
{{/each}}
<br>
  <button onclick="window.location='/game/new';">Add Game</button>
</div>

